<?php
$server = "localhost";
$database = "pjesme";
$username = "root";
$password = "";

$conn = mysqli_connect($server, $username, $password, $database);
$query = "SELECT * FROM Naziv";
$res = mysqli_query($conn, $query);
while($row = mysqli_fetch_array($res)){
    echo $row['link'];
}
mysqli_close($conn);
?>